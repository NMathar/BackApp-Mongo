{
  "name": "backappmongo",
  "version": "2.0.0",
  "description": "Backup your MongoDB Databases",
  "author": "NMathar",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "nuxt dev",
    "cron:start": "forever start server/cron.js && touch server/cron.lock",
    "cron:stop": "forever stop server/cron.js && rm -f server/cron.lock",
    "cron:restart": "forever restart server/cron.js",
    "cron:status": "test -f server/cron.lock && printf running || printf stopped",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "generate:key": "./create_key.sh",
    "prestart": "./create_key.sh"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.9.7",
    "@nuxtjs/dotenv": "^1.4.1",
    "@voerro/vue-tagsinput": "^2.2.0",
    "ale-mongoutils": "^1.0.3",
    "body-parser": "^1.19.0",
    "bootstrap-vue": "^2.11.0",
    "express": "^4.17.1",
    "forever": "^2.0.0",
    "lowdb": "^1.0.0",
    "mongodb": "^3.5.5",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.4.6",
    "nuxt": "^2.12.2",
    "rimraf": "^3.0.2",
    "simple-crypto-js": "^2.2.0",
    "uuid": "^7.0.3",
    "zip-a-folder": "^0.0.12"
  },
  "devDependencies": {}
}
